{
    "name": "nexendrie/nexendrie",
    "description": "Nexendrie website",
    "keywords": [
        "online", "game", "browser", "medieval", "renaissance", "fantasy"
    ],
    "homepage": "https://www.nexendrie.cz/",
    "license": "proprietary",
    "type": "project",
    "authors": [
        {
            "name": "Jakub Konečný",
            "email": "jakub.konecny2@centrum.cz",
            "role": "Architect"
        }
    ],
    "support": {
        "source": "https://gitlab.com/nexendrie/nexendrie",
        "issues": "https://gitlab.com/nexendrie/nexendrie/issues"
    },
    "require": {
        "php": ">=8.1.0",
        "ext-simplexml": "*",
        "ext-json": "*",
        "nette/application": "^3.1.10",
        "nette/bootstrap": "^3.1.4",
        "nette/caching": "^3.1.4",
        "nette/di": "^3.1.2",
        "nette/forms": "^3.1.11",
        "nette/security": "^3.1",
        "nette/utils": "^3.2.10",
        "tracy/tracy": "^2.9.8",
        "latte/latte": "^2.11.7",
        "kdyby/autowired": "^2.1.1",
        "nextras/orm": "^5.0",
        "ijvo/cronner": "^3.0.3",
        "nexendrie/event-calendar": "^0.7",
        "nextras/form-components": "^1.1.2",
        "nexendrie/book-component": "^2.3",
        "nexendrie/translation": "^2.3.1",
        "nexendrie/rss": "^0.11",
        "nexendrie/menu": "^2.4",
        "nexendrie/utils": "^1.9",
        "symfony/options-resolver": "^6.4",
        "heroesofabenez/chat": "^2.1",
        "heroesofabenez/combat": "dev-master",
        "nexendrie/rest-route": "^5.0",
        "robmorgan/phinx": "^0.12.12",
        "nette/http": "3.1.2",
        "eluceo/ical": "^2.13"
    },
    "require-dev": {
        "nexendrie/code-quality": "dev-master",
        "dg/ftp-deployment": "3.3.1",
        "phpstan/phpstan-nette": "^2.0",
        "nextras/orm-phpstan": "^2.0"
    },
    "replace": {
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/yaml": "*",
        "cakephp/database": "*"
    },
    "autoload": {
        "psr-4": {
            "Nexendrie\\": [
                "app/"
            ]
        },
        "classmap": ["app/Model/exceptions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Nexendrie\\": [
                "tests/Nexendrie/"
            ]
        }
    },
    "config": {
        "classmap-authoritative": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
