{
    "name": "nexendrie/nexendrie",
    "description": "Nexendrie website",
    "keywords": [
        "online", "game", "browser", "medieval", "renaissance", "fantasy"
    ],
    "homepage": "https://www.nexendrie.cz/",
    "license": "proprietary",
    "type": "project",
    "authors": [
        {
            "name": "Jakub Konečný",
            "email": "jakub.konecny2@centrum.cz",
            "role": "Architect"
        }
    ],
    "support": {
        "source": "https://gitlab.com/nexendrie/nexendrie",
        "issues": "https://gitlab.com/nexendrie/nexendrie/issues"
    },
    "require": {
        "php": ">=7.3.0",
        "ext-simplexml": "*",
        "nette/application": "~2.4",
        "nette/bootstrap": "~2.4",
        "nette/caching": "~2.5",
        "nette/di": "~2.4.10",
        "nette/forms": "~2.4",
        "nette/security": "~2.4",
        "nette/utils": "~2.5",
        "tracy/tracy": "~2.6",
        "latte/latte": "~2.7",
        "kdyby/autowired": "^1.5",
        "nextras/orm": "^3.1",
        "stekycz/cronner": "^2.2",
        "konecnyjakub/event-calendar": "^0.3.1",
        "nextras/forms": "^2.0.5",
        "nexendrie/book-component": "^2.0",
        "nexendrie/translation": "^1.2",
        "nexendrie/rss": "^0.7.1",
        "nexendrie/menu": "^2.1",
        "nexendrie/utils": "^1.6.1",
        "symfony/options-resolver": "^4.3",
        "heroesofabenez/chat": "^1.0",
        "heroesofabenez/combat": "^1.0",
        "adamstipak/nette-rest-route": "^4.0.1",
        "robmorgan/phinx": "^0.11.1"
    },
    "require-dev": {
        "nexendrie/code-quality": "dev-master",
        "dg/ftp-deployment": "^3.3",
        "phpstan/phpstan-nette": "^0.12"
    },
    "replace": {
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/yaml": "*",
        "cakephp/database": "*"
    },
    "autoload": {
        "psr-4": {
            "Nexendrie\\": [
                "app/"
            ]
        },
        "classmap": ["app/Model/exceptions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Nexendrie\\": [
                "tests/Nexendrie/"
            ]
        }
    },
    "config": {
        "classmap-authoritative": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
