{
    "name": "nexendrie/nexendrie",
    "description": "Nexendrie website",
    "keywords": [
        "online", "game", "browser", "medieval", "renaissance", "fantasy"
    ],
    "homepage": "https://www.nexendrie.cz/",
    "license": "proprietary",
    "type": "project",
    "authors": [
        {
            "name": "Jakub Konečný",
            "email": "jakub.konecny2@centrum.cz",
            "role": "Architect"
        }
    ],
    "support": {
        "source": "https://gitlab.com/nexendrie/nexendrie",
        "issues": "https://gitlab.com/nexendrie/nexendrie/issues"
    },
    "require": {
        "php": ">=7.4.0",
        "ext-simplexml": "*",
        "nette/application": "^3.1.2",
        "nette/bootstrap": "^3.1.1",
        "nette/caching": "^3.1.1",
        "nette/di": "^3.0.8",
        "nette/forms": "^3.1.3",
        "nette/security": "^3.1",
        "nette/utils": "^3.2",
        "tracy/tracy": "^2.8.4",
        "latte/latte": "^2.10.3",
        "kdyby/autowired": "^2.1.1",
        "nextras/orm": "^4.0.2",
        "ijvo/cronner": "^3.0.3",
        "nexendrie/event-calendar": "^0.7",
        "nextras/form-components": "^1.0",
        "nexendrie/book-component": "^2.3",
        "nexendrie/translation": "^2.3.1",
        "nexendrie/rss": "^0.10",
        "nexendrie/menu": "^2.4",
        "nexendrie/utils": "^1.8",
        "symfony/options-resolver": "^5.1",
        "heroesofabenez/chat": "^2.1",
        "heroesofabenez/combat": "^1.0",
        "adamstipak/nette-rest-route": "dev-master",
        "robmorgan/phinx": "^0.12.5",
        "nette/http": "3.1.2"
    },
    "require-dev": {
        "nexendrie/code-quality": "dev-master",
        "dg/ftp-deployment": "3.3.1",
        "phpstan/phpstan-nette": "^1.0",
        "nextras/orm-phpstan": "^1.0.1"
    },
    "replace": {
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/yaml": "*",
        "cakephp/database": "*"
    },
    "autoload": {
        "psr-4": {
            "Nexendrie\\": [
                "app/"
            ]
        },
        "classmap": ["app/Model/exceptions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Nexendrie\\": [
                "tests/Nexendrie/"
            ]
        }
    },
    "config": {
        "classmap-authoritative": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
