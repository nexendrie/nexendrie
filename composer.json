{
    "name": "nexendrie/nexendrie",
    "description": "Nexendrie website",
    "keywords": [
        "online", "game", "browser", "medieval", "renaissance", "fantasy"
    ],
    "homepage": "https://nexendrie.cz/",
    "license": "proprietary",
    "type": "project",
    "authors": [
        {
            "name": "Jakub Konečný",
            "email": "jakub.konecny2@centrum.cz",
            "role": "Architect"
        }
    ],
    "support": {
        "source": "https://gitlab.com/nexendrie/nexendrie",
        "issues": "https://gitlab.com/nexendrie/nexendrie/issues"
    },
    "require": {
        "php": ">=7.1.0",
        "nette/application": "~2.4",
        "nette/bootstrap": "~2.4",
        "nette/caching": "~2.4",
        "nette/di": "~2.4",
        "nette/forms": "~2.4",
        "nette/security": "~2.4",
        "nette/utils": "~2.4",
        "tracy/tracy": "~2.4",
        "latte/latte": "~2.4",
        "kdyby/autowired": "^1.4",
        "nextras/orm": "^2.2",
        "stekycz/cronner": "^2.0",
        "konecnyjakub/event-calendar": "dev-master",
        "nella/forms-datetime": "^2.4",
        "nexendrie/book-component": "^1.2",
        "nexendrie/rss": "^0.3",
        "symfony/options-resolver": "^3.2"
    },
    "require-dev": {
        "nexendrie/code-quality": "dev-master",
        "nette/tester": "dev-master",
        "dg/ftp-deployment": "^2.6",
        "phpstan/phpstan-nette": "^0.6.2"
    },
    "replace": {
      "satooshi/php-coveralls": "*"
    },
    "autoload": {
        "psr-4": {
            "Nexendrie\\": [
                "app/"
            ]
        },
        "classmap": ["app/Model/exceptions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Nexendrie\\": [
                "tests/"
            ]
        }
    },
    "config": {
        "classmap-authoritative": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
